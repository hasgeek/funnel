<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {%- for sitemap in sitemaps %}
  <sitemap>
    <loc>{{ sitemap.loc }}</loc>
    {%- if sitemap.lastmod %}
    <lastmod>{{ sitemap.lastmod.isoformat() }}</lastmod>
    {%- endif %}
  </sitemap>
  {%- endfor %}
</sitemapindex>
